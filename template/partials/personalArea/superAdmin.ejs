    <div class="block">
    <p>Administrators and parents</p>
    <table border="0">
      <tr class="table__headRow">
        <th>â„–</th>
        <th>Email</th>
        <th>Position</th>
      </tr>
      <!--  -->
      <tr>
        <% for (var i=0; i< admins.length; i++) { %>
          <tbody>
            <tr>
              <td><%=i+1%></td>
              <% if(admins[i].access_level == 2) { %>
                <td><a href="/publicProfile/admins/id<%=admins[i].id%>"><%=admins[i].email%></a></td>
                <td>Administrator</td>
                <form action="/deleteAdmin/id<%=admins[i].id%>" method="POST" name="deleteAdminForm">
                  <td class="delete"><button class="deleteBtn" onclick="return confirmDelete();">Delete</button></td>
                </form>
                <% }  else { %>
                  <td><a href="/publicProfile/coaches/id<%=admins[i].id%>"><%=admins[i].email%></a></td>
                  <td>Parent</td>
                  <form action="/deleteCoach/id<%=admins[i].id%>" method="POST" name="deleteCoachForm">
                    <td class="delete"><button class="deleteBtn" onclick="return confirmDelete();">Delete</button></td>
                  </form>
                  <% } %>
                </tr>
              </tbody>
              <% } %>
            </tr>
          </table>
        <%-partial('../../partials/personalArea/pagination')%>
      </div>

      <div class="block">
          <h2>Create new administrator</h2>
          <form action="/addNewAdmin" method="POST" name="addNewAdminForm">
            <span class="error"></span>
            <label for="login">Login</label>
            <input type="email" name="email" value="" required>
            <label for="login">Password</label>
            <input type="password" name="password" value="" required>
            <button type="submit" name="button">Create</button>
          </form>
      </div>

<div class="settings">
    <h2>Settings</h2>

    <label>Back button
        <input type="range" min="0" max="2" name="back-btn" id="backBtnChanger">
    </label>

    <label>Next button
        <input type="range" min="0" max="2" name="next-btn" id="nextBtnChanger">
    </label>

    <label>Again button
        <input type="range" min="0" max="2" name="again-btn" id="againBtnChanger">
    </label>

    <label>Basket
        <input type="range" min="0" max="3" name="basket" id="basketChanger">
    </label>
    <label>Parnet
        <input type="range" min="0" max="3" name="parnet" id="parnetChanger">
    </label>
    <label>Status bar
        <input type="checkbox" name="statusBar" id="statusBarChanger">
        </label>
</div>